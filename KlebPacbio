#convert bax to bam files
bax2bam -o m161019 m161019_004147_00127_c101051402550000001823235612291634_s1_p0.1.bax.h5 m161019_004147_00127_c101051402550000001823235612291634_s1_p0.2.bax.h5 m161019_004147_00127_c101051402550000001823235612291634_s1_p0.3.bax.h5

cat bax2bam.txt | parallel -j 2 --col-sep "\t" --bar  "bax2bam -o {1} {2} {3} {4}"

